package test;

public class test {
	public static void main(String[] args){
		String str = "[ID: 4 | Marca: marca | Modelo: prueba | Precio: 0]";
		String[] str2 =str.split("\\s+");
		
		// \\S+Precio: //
		
		System.out.println("________________PRIMER SPLIT______________");
		for(int i=0; i<str2.length ; i++){
			
			System.out.println(i+"->"+str2[i]);
			
		}
		String[] str3 = str2[10].split("\\]");
		System.out.println("________________SEGUNDO SPLIT______________");
		for(int i=0 ; i<str3.length ; i++){
			System.out.println(i+"->"+str3[i]);
		}
	}
}
